.float-panel
  position: absolute
  background: #333
  box-shadow: 0 0 8px rgba(0,0,0,1)
  min-width: 28px
  .dragHandle
    position relative
    height: 24px
    background: #252525
  .float-panel-title
    color: #dedede
    font-size: 14px
    font-weight: 100
    line-height: 24px
    margin: 0 0 0 6px
  .icon-close
    position: absolute
    color: #fff
    right: 5px
    font-size: 18px
    top: 4px
    cursor: pointer
