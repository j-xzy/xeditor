{
  "presets": [
    [
      "@babel/env",
      {
        "targets": {
          "chrome": 52,
          
        }
      }
    ],
    "@babel/react"
  ],
  "plugins": [
    [
      "import",
      {
        "libraryName": "antd",
        "style": "css",
        "libraryDirectory": "es"
      }
    ],
    ["@babel/plugin-proposal-decorators", { "legacy": true }],
    ["@babel/plugin-proposal-class-properties", { "loose" : true }]
  ]
}